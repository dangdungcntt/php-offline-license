{
    "name": "nddcoder/php-offline-license",
    "description": "",
    "keywords": [
        "nddcoder",
        "php-offline-license"
    ],
    "homepage": "https://github.com/nddcoder/php-offline-license",
    "license": "MIT",
    "authors": [
        {
            "name": "Dung Nguyen Dang",
            "email": "dangdungcntt@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3",
        "vimeo/psalm": "^4.3"
    },
    "autoload": {
        "psr-4": {
            "Nddcoder\\PhpOfflineLicense\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nddcoder\\PhpOfflineLicense\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "psalm",
        "test": "phpunit --colors=always",
        "test-coverage": "phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
